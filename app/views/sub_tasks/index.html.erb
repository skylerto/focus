<p id="notice"><%= notice %></p>

<h1>Listing Sub Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Parent</th>
      <th>Title</th>
      <th>Progress</th>
      <th>Estimated progress</th>
      <th>Complete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_tasks.each do |task| %>
    <tr>
      <td><%= task.task_id %></td>
      <td><%= task.title %></td>
      <td><%= task.progress %></td>
      <td><%= task.estimated_progress %></td>
      <td><%= task.complete %></td>
      <td><%= link_to 'Show', task_sub_task_url(:id => task.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table><br>
